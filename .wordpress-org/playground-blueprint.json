{
	"$schema": "https://playground.wordpress.net/blueprint-schema.json",
	"landingPage": "/wp-admin/edit.php?post_type=page",
	"preferredVersions": {
		"wp": "6.6",
		"php": "8.2"
	},
	"steps": [
		{
			"step": "installPlugin",
			"pluginZipFile": {
				"resource": "url",
				"url": "https://raw.githubusercontent.com/WEBLAZER/any-block-carousel-slider/main/.wordpress-org/any-block-carousel-slider-playground.zip"
			}
		},
		{
			"step": "activatePlugin",
			"pluginName": "any-block-carousel-slider/any-block-carousel-slider.php"
		},
		{
			"step": "runWpCli",
			"command": "wp site empty --yes --uploads"
		},
		{
			"step": "importWxr",
			"file": {
				"resource": "url",
				"url": "https://raw.githubusercontent.com/WEBLAZER/any-block-carousel-slider/main/.wordpress-org/demo-content.xml"
			}
		},
		{
			"step": "setSiteOptions",
			"options": {
				"blogname": "Any Block Carousel Slider Demo",
				"blogdescription": "Demo site showcasing the Any Block Carousel Slider plugin"
			}
		}
	]
}